	-fx-font-family: "Roboto", "Arial", sans-serif;
}

/* cor background ao chamar o programa */
.button {
     -fx-background-color	: -fx-primaria-escura; /* verde #027939 */ /*azul escuro #1E73BE */
     -fx-text-fill			: -fx-branco; 
     -fx-text-base-color	: -fx-branco;
     -fx-font-weight		: bold;
}
/* ao passar o mouse 
	muda a cor do background no combo box DD DMS UTM ao passa o mouse*/
.button:hover { 
    -fx-background-color	: -fx-branco;
    -fx-text-fill			: -fx-primaria-escura; /*azul escuro = muda cor da letra do botao ao passar mouse */
    -fx-text-base-color		: -fx-primaria-escura;
}

/* ao passar o mouse muda cor do texto */
.button:pressed:hover { 
    -fx-scale-y				: 1.1;
    -fx-scale-x				: 1.1;
    -fx-text-base-color		: -fx-primaria-escura; 
    
}

