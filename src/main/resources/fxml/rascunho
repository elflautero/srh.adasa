/* cor de fundo tab, local do click */
.tab { 
	-fx-background-color	: -fx-branco; 	/*rgba(71,169,100,1);*/ /* escuro S */
	-fx-border-radius		: 0 0 0 0;		/* tabs quadradas */
	-fx-background-radius	: 0 0 0 0;
}

/* cor ao passar o mouse */
.tab:hover { 
    -fx-background-color	: -fx-primaria-escura; /*rgba(71,169,100,1);*/

}
/* cor ao selecionar */
.tab:selected { 
	-fx-background-color	: -fx-primaria-escura; /*rgba(71,169,100,1);*/
}

/* para o icon  */
.tab .glyph-icon { 
	-fx-fill				: -fx-preto; /* icon nas tabs na cor preta */
}

/* cor do icone ao passar o mouse */
.tab:hover .glyph-icon { 
	-fx-fill				: -fx-branco; /* icon nas tabs ao passar o mouse na cor branca */
}
/* cor do icone ao selecionar */
.tab:selected .glyph-icon { 
	-fx-fill				: -fx-branco; /* icon nas tabs ao selecionar na cor branca */
}

 /* cor ao passar o mouse do texto da tab */
.tab:hover .tab-label {
    -fx-background-color	: -fx-primaria-escura; /*rgba(71,169,100,1);*/
    -fx-text-fill			: -fx-branco;
}

 /* cor do texto da tab ao selecionar */
.tab:selected .tab-label  {
    -fx-background-color	: -fx-primaria-escura; /*rgba(71,169,100,1);*/
    -fx-text-fill			: -fx-branco;
}

/* para o fundo do tabPane ficar transparent, aparecer somente a tab para escolha */
.tab-pane .tab-header-area .tab-header-background { 
    -fx-background-color	: transparent	; /* fundo */
    -fx-border-color		: -fx-primaria-escura; /*rgba(71,169,100,1);*/		/* cor da borda */
  	-fx-border-radius		: 2 2 2 2;					/* arredondamento dos cantos */
  	-fx-border-width		: 0 0 1 0; 						/* somente parte inferior aparecendo da borda */
}

.border-pane {
	-fx-background-color	: transparent	;
}

